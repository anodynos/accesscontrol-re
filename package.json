{
  "name": "accesscontrol-re",
  "version": "3.1.0",
  "main": "./build/src/index.js",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "dev": "npm-run-all clean build:watch & (sleep 5 && npm run test:watch)",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "tslint": "tslint --fix -p .",
    "prettier": "prettier --write \"**/*.{ts,js,json}\"",
    "format": "npm-run-all prettier tslint prettier"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "accesscontrol": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.14.18",
    "accesscontrol": "^2.2.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "rimraf": "^2.7.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tsutils": "^3.17.1",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">= 6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "postcommit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "Angelos Pikoulas",
  "license": "MIT",
  "types": "./build/src/"
}
